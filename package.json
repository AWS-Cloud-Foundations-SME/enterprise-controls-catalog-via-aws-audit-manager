{
    "name": "audit-manager-blog",
    "version": "0.1.0",
    "bin": {
        "audit-manager-blog": "bin/audit-manager-blog.js"
    },
    "scripts": {
        "lint": "eslint --fix --ext .ts .",
        "build:lambda": "cd lambda && npm run build && cd ..",
        "build:infra": "tsc",
        "build": "npm run build:lambda && npm run build:infra",
        "synth": "cdk synth -q",
        "watch": "tsc -w",
