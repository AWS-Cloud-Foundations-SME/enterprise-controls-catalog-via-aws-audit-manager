        "cdk": "cdk",
        "all": "npm run lint && npm run build && npm run test && npm run synth"
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.108.0",
        "@types/jest": "^26.0.24",
        "@types/node": "10.17.27",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "aws-cdk": "1.108.0",
        "aws-lambda": "^1.0.6",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-config-typescript": "^3.0.0",
        "eslint-plugin-header": "^3.1.0",
        "eslint-plugin-prettier": "^3.3.0",
        "jest": "^26.4.2",
        "prettier": "^2.3.2",
        "ts-jest": "^26.2.0",
        "ts-loader": "^9.2.3",
        "ts-node": "^9.0.0",
        "typescript": "~3.9.7",
        "webpack": "^5.50.0"
    },
    "dependencies": {
        "@aws-cdk/aws-lambda": "^1.108.0",
        "@aws-cdk/aws-s3": "^1.108.0",
        "@aws-cdk/aws-s3-notifications": "^1.108.0",
        "@aws-cdk/aws-sns": "^1.108.0",
        "@aws-cdk/core": "1.108.0",
